#include<bits/stdc++.h>
using namespace std;
const int N =130;
int vis1[N],vis2[N],vis[N],dp[N];
struct node
{
    char front[15],back[15];
}s[N];
 
int main()
{
    int n;cin>>n; 
    for(int i=1;i<=n;i++)
        scanf("%s %s",s[i].front,s[i].back);
    for(int i=1;i<=n;i++)
    {
        for(int j='a';j<='z';j++)
        {
            vis1[j]=0;vis2[j]=0;vis[j]=0;   
        }   
        int len1=strlen(s[i].front),len2=strlen(s[i].back);
        for(int k=0;k<len1;k++)
            vis1[s[i].front[k]]++;
        for(int k=0;k<len2;k++)
            vis2[s[i].back[k]]++;
        for(int j='a';j<='z';j++)
        {
            vis[j]=max(vis1[j],vis2[j]);
            dp[j]=max(dp[j],vis[j]+dp[j]);
        }
    }
    for(int i='a';i<='z';i++)
        printf("%d\n",dp[i]);
    return 0;
}
