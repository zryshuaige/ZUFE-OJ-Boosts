#include<iostream>
using namespace std;
const int N = 205;
int a[N],dp[N];
int main()
{
    int n,ans=0;
    cin>>n;
    for(int i =1;i<=n;i++)  
    {
        cin>>a[i];
        dp[i]=1;
    }
    for(int i=2;i<=n;i++)
        for(int j=1;j<i;j++)
            if(a[j]<a[i])   dp[i]=max(dp[i],dp[j]+1);
    for(int i=1;i<=n;i++)   
        if(dp[i]>ans)   ans=dp[i];   
    cout<<"max="<<ans;
    return 0;
}
