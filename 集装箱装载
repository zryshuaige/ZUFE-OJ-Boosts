#include<iostream>
using namespace std;
const int N = 55;
int dp[N][1005],w[N];

int main()
{
    int n,c;
    cin>>n>>c;
    for(int i=1;i<=n;i++)
        cin>>w[i];
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=c;j++)
        {
            if(j>=w[i]) dp[i][j]=max(dp[i-1][j],dp[i-1][j-w[i]]+w[i]);
            else dp[i][j]=dp[i-1][j];
        }
    }
    cout<<dp[n][c];
    return 0;
}
