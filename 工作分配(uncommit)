#include<bits/stdc++.h>
using namespace std;
const int N =25;
int a[N][N],vis[N];
int n,cnt=0,minf=1e9;
void dfs(int x)//x表示工序
{
    if(x>=n)
    {
        if(cnt<minf){
            minf=cnt;
            return;
        }
    }
     
    for(int i=1;i<=n;i++)
    {
        if(!vis[i])
        {
            vis[i]=1;
            cnt+=a[x][i];
            if(cnt<minf) dfs(x+1);
            vis[i]=0;
            cnt-=a[x][i];   
        }
    }
}
 
int main()
{
    cin>>n;
    for(int i=1;i<=n;i++)
        for(int j=1;j<=n;j++)
            cin>>a[i][j];
    dfs(1);
    cout<<minf;
    return 0;
}
