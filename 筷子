#include<bits/stdc++.h>
#include<math.h>
using namespace std;
const int N = 105;
int a[N],dp[N][N];//dp[][]表示N中选择N. 

int cmp(int x,int y)
{
	return x>y;
}

int main()
{
	int n,k;
	cin>>n>>k;
	k+=3;
	for(int i=1;i<=n;i++) cin>>a[i];
	for(int i=0;i<=n;i++)
	{
		dp[i][0]=0;
		for(int j=1;j<=k;j++)	dp[i][j]=1e8;
	 } 
	if(n<2*k){
		cout<<-1;
		return 0;	
	}
	else 
	{	
		sort(a+1,a+n+1,cmp);
		for(int i=2;i<=n;i++)
		{
			for(int j=1;j<=k;j++)
			{
				dp[i][j]=min(dp[i-1][j],dp[i-2][j-1]+(a[i]-a[i-1])*(a[i]-a[i-1]));
			} 
		}
	}
	cout<<dp[n][k];
	return 0;
}
