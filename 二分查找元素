#include<bits/stdc++.h>
using namespace std;
const  int N =1e5+5;
int a[N],n;

int bitsearch(int a[],int x)
{
	int l=1,r=n,mid;
	while(l<=r)
	{
		mid=(l+r)/2;
		if(a[mid]>=x)
			r=mid-1;
		else l=mid+1;
	}
	return l;
}
int main()
{
	int m,x;
	scanf("%d%d",&n,&m);
	for(int i=1;i<=n;i++)	scanf("%d",&a[i]);
	sort(a+1,a+1+n);
	while(m--)
	{
		scanf("%d",&x);
		int p=bitsearch(a,x);
		if(p==n-1)	printf("%d\n",n+1);
		else printf("%d\n",p);    
	}
	return 0;
}
