#include<bits/stdc++.h>
#include<string.h>
using namespace std;
const int N =25;
string s1,s2;

void dfs(int l1,int r1,int l2,int r2)
{
	char ch=s2[r2];int pos;
	if(l1>r1)return;
	if(l2>r2)return;
	for(int i=l1;i<=r1;i++)
	{
		if(s1[i]==ch)
		{
			cout<<ch;
			pos=i;break;
		}
	}
	dfs(l1,pos-1,l2,l2+(pos-l1-1));
	dfs(pos+1,r1,l2+pos-l1,r2-1);
}

int main()
{
	cin>>s1;cin>>s2;
	int len1=s1.size(),len2=s2.size();
	dfs(0,len1-1,0,len2-1);
	return 0;
}
