#include<bits/stdc++.h>
using namespace std;
const int N = 1e5+5;
int  a[N],b[5];

int  bitsearch(int a[],int k,int n)
{
	int l=1,r=n;
	while(l<=r)
	{
		int mid=(l+r)/2;
		if(a[mid]>=k)	r=mid-1;
		else l=mid+1;
	}
	return l;
}
int main(void)
{
	int n,q,k;
	cin>>n>>q;
	for(int i=1;i<=n;i++)	cin>>a[i];
	sort(a+1,a+1+n);
	while(q--)
	{
		cin>>k;
		b[1]=bitsearch(a,k,n);
		b[2]=bitsearch(a,k+1,n)-1;
		if(b[1]<1||b[2]>n) cout<<"-1 -1\n";
		
		else if(a[b[1]]==a[b[2]]&&a[b[1]]==k)
		    cout<<b[1]-1<<" "<<b[2]-1<<"\n";
		else cout<<"-1 -1\n";
	}
	return  0;
}
