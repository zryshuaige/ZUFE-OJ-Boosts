#include<iostream>
using namespace std;
const int N = 105;
int dp[25][25];//dp[i] 
int w[25],v[25];
int main()
{
	int n,m;
	cin>>n;
	cin>>m;
	for(int i=1;i<=n;i++)
		cin>>w[i]>>v[i];
	for(int i=1;i<=n;i++)
	{
		for(int j=1;j<=m;j++)
		{
			if(j>w[i]) dp[i][j]=max(dp[i-1][j],dp[i-1][j-w[i]]+v[i]);
			else dp[i][j]=dp[i-1][j];
		}
	}
	cout<<dp[n][m];
	return 0;
}
